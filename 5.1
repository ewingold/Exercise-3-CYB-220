# establishing variable
car = 'subaru'

# First test, is car equal to 'subaru'?
print("Is car == 'subaru'? I predict True.")
print(car == 'subaru')

# Second test, is car equal to 'audi'?
print("\nIs car == 'audi'? I predict False.")
print(car == 'audi')

# Third test, Is car not equal to 'toyota'?
print("\nIs car != 'toyota'? I predict True.")
print(car != 'toyota')

# Fourth test, Is car equal to case-sensitive 'Subaru'?
print("\nIs car == 'Subaru'? I predict False.")
print(car == 'Subaru')

# Fifth test, Is car not equal to case-sensitive 'Subaru'?
print("\nIs car != 'Subaru'? I predict True.")
print(car != 'Subaru')

# Sixth test, Is car equal to 'subaru' and not equal to 'audi'?
print("\nIs car == 'subaru' and car != 'audi'? I predict True.")
print(car == 'subaru' and car != 'audi')

# Seventh test, Is car equal to 'subaru' or equal to 'audi'?
print("\nIs car == 'subaru' or car == 'audi'? I predict True.")
print(car == 'subaru' or car == 'audi')

# Eighth test, is car equal to 'subaru' or equal to 'chevrolet'?
print("\nIs car == 'subaru' or car == 'chevrolet'? I predict True.")
print(car == 'subaru' or car == 'chevrolet')

# Ninth test, Is caar not equal to 'subaru' and not equal to 'chevrolet'?
print("\nIs car != 'subaru' and car != 'chevrolet'? I predict False.")
print(car != 'subaru' and car != 'chevrolet')

# Tenth test, Is car equal to case-sensitive 'Subaru'?
print("\nIs car.lower() == 'subaru'? I predict True.")
print(car.lower() == 'subaru')



